---
import Layout from '../layouts/Layout.astro';
import Footer from '../layouts/Footer.astro';
import Header from '../layouts/Header.astro';
import EdenSection from '../layouts/EdenSection.astro';
import '../styles/style.css'
// console.log('here ', Astro.url)
import { AlignLeft, ArrowLeft, ArrowRight } from 'lucide-react';
let links = ['hero', 'hey-there', 'table', 'quick-overview', 'quick-overview-2', 'problem', 'research-and-insights', 'design-process', 'foundation', 'life', 'visual', 'fidelity', 'impact-and-feedback', 'collaboration', 'reflections', 'closing']
let data = {active: 'hero', links: ['eden', 'hey-there', 'table', 'quick-overview', 'quick-overview-2', 'research-and-insights', 'problem', 'user-research', 'market-research', 'competitive', 'design-process', 'the-brand', 'navigation', 'ui-systems', 'no-scrolling', 'bento', 'dynamic-island', 'soft-color', 'calender', 'image', 'key-features', 'features-list', 'market-strategy', 'market-focus', 'product-roadmap', 'conclusion', 'conclusion-2', 'contact']}

---

<Layout title="Welcome to Astro.">
	<!-- nav -->
    <div  x-data={JSON.stringify(data)} class="body-cont">
    <Header
        active="Eden"
    >

        <!-- lg:absolute lg:opacity-0 lg:-top-[1000px]  -->
        <div class="h-2 flex gap-4 max-w-[700px] w-[40%] md:w-60% lg:w-[80%] overflow-hidden mob-timeline" slot="timeline">
            {
                data.links.map(link => 
                    <a href={"#"+link}  class="mob-timeline-indicator h-2 min-w-5 sm:min-w-5 lg:min-w-4 bg-gray-200 rounded-full"></a>
                )
            }
        </div>
        
    </Header>
    <!-- bottom-nav -->
    <div class="fixed bottom-0 right-0 z-10 flex bg-white/80 p-4 xl:px-10 lg:pl-10 py-6 xl:py-10 bottom-nav">
        <ul class="flex gap-4 text-[#afafaf] font-semibold sm:text-[10px] text-[6px] lg:text-sm">
            <li @click="active = 'quick-overview'" :class="{'text-black' : ['quick-overview', 'quick-overview-2'].includes(active)}"><a href="#quick-overview">Quick Overview</a></li>
            <li @click="active = 'research-and-insights'" :class="{'text-black' : ['research-and-insights', 'problem', 'design-insights', 'competitive', 'user-research', 'market-research'].includes(active)}"><a href="#research-and-insights"> Research and Insights</a></li>
            <li @click="active = 'design-process'" :class="{'text-black' : ['design-process', 'the-brand', 'navigation', 'ui-systems', 'no-scrolling', 'bento', 'dynamic-island', 'soft-color', 'calender', 'image'].includes(active)}"><a href="#design-process"> Design and Product Decisions</a></li>
            <li @click="active = 'key-features'" :class="{'text-black' : ['key-features', 'features-list'].includes(active)}"><a href="#key-features">Key Features</a></li>
            <li @click="active = 'market-strategy'" :class="{'text-black' : ['market-strategy', 'market-focus', 'product-roadmap'].includes(active)}"><a href="#market-strategy">Market Strategy</a></li>
            <li @click="active = 'conclusion'" :class="{'text-black' : ['conclusion', 'conclusion-2'].includes(active)}"><a href="#conclusion">Conclusion</a></li>

        </ul>
    </div>

    <!-- arrow -->
    <!-- <a :href="'#' + (links[links.indexOf(active) + 1] || links[links.length])" @click="setTimeout(()=> {active = (links[links.indexOf(active) + 1] || links[links.length])}, 1) " class="h-[50px] aspect-[2/4] bg-center bg-contain bg-no-repeat bg-[url('/icons/down_arrow.svg')] fixed left-5 bottom-2 z-[1] down-arrow"></a>
    <a :href="'#' + (links[links.indexOf(active) - 1] || links[0])" @click="setTimeout(()=> {active = (links[links.indexOf(active) - 1] || links[0])}, 1) " class="h-[100px] aspect-[2/4] bg-center bg-contain bg-no-repeat bg-[url('/icons/down_arrow.svg')] fixed left-20 bottom-5 z-[1] up-arrow hidden"></a> -->


	<main>
		<!-- hero -->
       <EdenSection hide/>
		

        <!-- hey there -->
        <section id="hey-there" class="lg:px-20 px-4 pt-20 pb-28 min-h-screen flex items-center justify-center relative max-w-[1700px] mx-auto">
            <h2 class="text-sm sm:text-lg md:text-2xl xl:text-3xl leading-[35px] lg:leading-[48px] xl:leading-[72px] max-w-[1116px] w-[80%] text-[#AFAFAF] quick-appear">
                This document is going to be mostly about the <span class="text-black ">design</span>
                <span class="text-black ">decisions</span> behind some of the <span class="text-black ">features</span> included in <span class="text-black ">Eden</span>
                along with the <span class="text-black ">market strategy</span> guiding how it was built
                to <span class="text-black ">grow</span>.
            </h2>
			<!-- <div class="h-[100px] aspect-[2/4] bg-center bg-contain bg-no-repeat bg-[url('/icons/down_arrow.svg')] absolute left-20 bottom-15"></div> -->
            <!-- <p class="absolute right-20 bottom-[20%] w-[70%] text-right quick-appear">“Wow! Just Wow!! I don’t even know what I’ll change. It’s so fun & easy to use.” - Tunde</p> -->
        </section>

        <!-- table of contents -->
        <section id="table" class="lg:px-20 px-4 min-h-screen relative pt-20 pb-28 max-w-[1700px] mx-auto overflow-hidden">
            <h2 class="font-safira lg:text-sm xl:text-2xl heading-up">Content</h2>
            {/* Updated grid-rows to match the 6 content items */}
            <div class="max-w-2xl grid grid-cols-[80%_20%] grid-rows-6 mt-20 mx-auto my-auto gap-4 lg:text-xl xl:text-2xl">
                {/* Item 1 - Matches image */}
                <p class="quick-right">Quick Overview</p><p class="quick-up">01</p>
                {/* Item 2 - Matches image */}
                <p class="quick-right">Research and Insights</p><p class="quick-up">02</p>
                {/* Item 3 - Text updated to match image */}
                <p class="quick-right">Design and Product Decisions</p><p class="quick-up">03</p>
                {/* Item 4 - Text updated to match image */}
                <p class="quick-right">Key Features</p><p class="quick-up">04</p>
                {/* Item 5 - Added from image */}
                <p class="quick-right">Market Strategy</p><p class="quick-up">05</p>
                {/* Item 6 - Added from image */}
                <p class="quick-right">Conclusion</p><p class="quick-up">06</p>
            </div>
            </section>

        <!-- 01 -->
        <div id="quick-overview" class="hide-bottom-nav bg-black text-white">
            <section class="h-screen overflow-hidden grid grid-cols-2 pt-20 pb-28 max-w-[1700px] mx-auto lg:px-20 px-4 relative gap-4 xl:gap-6">
                <!-- project description and actions -->
                <div class="max-w-[700px] flex flex-col justify-center gap-4">
                    <h1 class="text-4xl font-cookie quick-down">01</h1>
                    <h2 class="font-safira text-sm  md:text-xl 2xl:text-5xl leading-[30px] xl:leading-[80px] 2xl:leading-[80px] my-2 quick-down">Quick Overview</h2>
                </div>
    
                <!-- image -->
                <!-- <div class="flex items-center justify-end p-20 w-full quick-up bg-[url('/images/pencil_hoops/sections/k')] bg-blend-color-burn bg-white bg-center bg-no-repeat bg-cover h-full"></div> -->
    
                <!-- <div class="h-[100px] aspect-[2/4] bg-center bg-contain bg-no-repeat bg-[url('/icons/down_arrow.svg')] absolute right-20 bottom-15"></div> -->
            </section>
        </div>
		

        <!-- 01 - Quick overview -->
        <section id="quick-overview-2" class="h-auto lg:min-h-screen overflow-hidden flex flex-col lg:grid pt-20 pb-28 lg:px-20 px-4 xl:grid-cols-[auto_auto_auto] lg:grid-cols-[auto_auto_10%] grid-rows-[auto_auto_auto] lg:gap-4 xl:gap-10 relative max-w-[1700px] mx-auto">
            <h3 class="text-lg mt-8 mb-1 border-gray-200 border-b-[1px] lg:border-b-[0px] lg:mt-0 lg:text-xl xl:text-2xl pb-2 xl:py-4">Product Overview</h3>
             {/* Updated parent <p> with flex for spacing, removed <br> tags */}
            <p class="md:w-full max-w-[900px] border-gray-200 col-span-2 text-base  flex flex-col gap-4">
                {/* Paragraph 1 wrapped in span */}
                <span>
                    Eden is a simple, thoughtful Al-powered financial assistant. It's designed to be a calm space where money grows steadily, clearly, and without pressure.
                </span>
                 {/* Paragraph 2 wrapped in span */}
                <span>
                    Unlike most financial tools that <strong>overwhelm with data and dashboards</strong>, Eden helps people manage spending, track investments, and understand their finances in a way that <strong>feels natural and empowering</strong>.
                </span>
                 {/* Paragraph 3 wrapped in span */}
                <span>
                    The idea came from my time working at the Royal Bank of Canada, where I saw firsthand how <strong>stressed and unsure many clients felt</strong> when dealing with their money. I wanted to create something that <strong>felt more human, less noisy, and built on real trust</strong>.
                </span>
            </p>
            <h3 class="text-lg mt-8 mb-1 border-gray-200 border-b-[1px] lg:border-b-[0px] lg:mt-0 lg:text-xl xl:text-2xl pb-2 xl:py-4 lg:pt-8">My Role</h3>
            {/* My Role section remains unchanged from previous correct version */}
            <div class="md:w-full max-w-[900px] lg:border-y-[1px] lg:py-8 border-gray-200 col-span-2 flex flex-col gap-4 text-base ">
                <p>
                    I led the design work for Eden and was involved in everything, from early ideas to the final screens. My work included:
                </p>
                <ul class="list-disc pl-5 flex flex-col gap-4">
                    <li>
                        <strong>Designing the product:</strong> I created the full experience for desktop and mobile, keeping it simple and easy to use.
                    </li>
                    <li>
                        <strong>Building the design system:</strong> I made reusable components and clean layouts that work in both light and dark mode.
                    </li>
                    <li>
                        <strong>Creating the brand:</strong> I came up with the name, logo, and look of Eden so it feels calm and trustworthy.
                    </li>
                    <li>
                        <strong>Doing the research:</strong> I looked into what users needed, studied other finance tools, and found what was missing.
                    </li>
                    <li>
                        <strong>Exploring funding:</strong> I researched grants and programs in Canada and the U.S. that could support Eden's future and I built a roadmap for that.
                    </li>
                </ul>
                <p>
                    This project gave me the chance to design, research, and think about the big picture, all with the goal of <strong>building something people can trust with their money</strong>.
                </p>
            </div>
        </section>

		<!-- 02 -->
        <section id="research-and-insights" class="hide-bottom-nav bg-black text-white">
            <section class="h-screen overflow-hidden grid grid-cols-2 pt-20 pb-28 lg:px-20 px-4  relative max-w-[1700px] mx-auto">
                <!-- project description and actions -->
                <div class="max-w-[700px] flex flex-col justify-center gap-10">
                    <h1 class="text-4xl font-cookie quick-down">02</h1>
                    <h2 class="font-safira text-sm  md:text-xl 2xl:text-5xl leading-[30px] xl:leading-[80px] 2xl:leading-[80px] my-2 quick-down">Research and Insights</h2>
                    <!-- <p>Our design process was anchored in extensive user research, employing both primary and secondary research methodologies and incorporating qualitative and quantitative data to capture a wide range of consumer insights.</p> -->
                </div>
    
                <!-- image -->
                <!-- <div class="flex items-center justify-end p-20 w-full quick-up bg-[url('/images/pencil_hoops/sections/k')] bg-white bg-center bg-no-repeat bg-cover bg-blend-color-burn"></div> -->
    
                <!-- <div class="h-[100px] aspect-[2/4] bg-center bg-contain bg-no-repeat bg-[url('/icons/down_arrow.svg')] absolute right-20 bottom-15"></div> -->
            </section>
        </div>

        <!-- the problem -->
        <section id="problem" class="min-h-screen overflow-hidden flex flex-col gap-8 lg:gap-4 lg:grid lg:grid-cols-2 pt-20 pb-28 lg:px-20 px-4 relative max-w-[1700px] mx-auto">
            <div class="lg:max-w-[500px] xl:max-w-[600px] flex flex-col justify-center gap-2 md:gap-4 xl:gap-6">
                <h2 class="font-safira text-xl leading-[50px] lg:text-2xl xl:text-3xl my-2 xl:my-4">The Problem</h2>
                <p class="flex flex-col gap-4 text-base ">
                    {/* Content updated from image with bold tags */}
                    <span>
                        Eden wasn't always going to be what it is now. It started as a simple plan for a component library that developers could use in <strong>financial dashboards</strong>. But as I reflected on my year at the Royal Bank of Canada, the direction shifted.
                    </span>
                    <span>
                        I spoke to people every day about their money, I saw how <strong>overwhelmed they felt</strong>. Even the tools meant to help often left them more <strong>confused</strong> than before. On top of that, financial advisors at Scotiabank pointed out that even banks needed a <strong>better way to manage and track external investments</strong>. These conversations added to what I already knew from <strong>five years of working in finance</strong>.
                    </span>
                     <span>
                        Eden wasn't just going to be a set of building blocks for developers. It became something much bigger.
                    </span>
                </p>
            </div>
        
            <div class="lg:max-w-[500px] xl:max-w-[600px] flex flex-col justify-center gap-2 md:gap-4 xl:gap-6">
                <h2 class="font-safira text-xl leading-[50px] lg:text-2xl xl:text-3xl my-2 xl:my-4">The Goal</h2>
                <div class="flex flex-col gap-4 text-base ">
                     {/* Content updated from image with bold tags */}
                    <p>
                        My goal was to create something that flips the typical approach to financial apps. Most are designed for speed, attention, and short-term engagement. Eden is built for <strong>peace, trust, and long-term growth</strong>.
                    </p>
                    <p>
                         I wanted to design a tool that helps people feel <strong>confident and in control</strong>, not anxious or rushed. Eden adapts to you. It makes space for your goals, your pace, and your journey with money.
                    </p>
                    <div class="flex flex-col gap-4">
                        <p>Here's the issue I set out to solve:</p>
                        <ul class="list-disc pl-5 space-y-1 font-semibold flex flex-col gap-4">
                            <li>
                                Finance tools often feel loud, cluttered, or impersonal.
                            </li>
                            <li>
                                People don't just need charts and numbers, they need clarity, comfort, and a sense of control.
                            </li>
                             <li>
                                Most apps right now are built for quick engagement, not long-term support. I wanted to flip that idea.
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        
        </section>

         <!-- user research -->
         <section id="user-research" class="h-auto lg:min-h-screen overflow-hidden flex flex-col gap-8 lg:gap-4 lg:grid lg:grid-cols-2 pt-20 pb-28 lg:px-20 px-4 relative max-w-[1700px] mx-auto">

            <div class="lg:max-w-[500px] xl:max-w-[600px] flex flex-col justify-start gap-2 md:gap-4 xl:gap-6">
                <h2 class="font-safira text-xl leading-[50px] lg:text-2xl xl:text-3xl my-2 xl:my-4">User Research</h2>
                 {/* Container div for spacing paragraphs and lists */}
                <div class="flex flex-col gap-4 text-base ">
                    <p>
                        During my time at RBC, I listened to people talk about money every day. I noticed patterns—recurring frustrations that weren't being addressed:
                    </p>
                     {/* List with internal spacing AND entire items bolded */}
                    <ul class="list-disc pl-5 flex flex-col gap-4">
                        <li>
                            <strong>Users missed payments not because they didn't have money, but because the funds were in the wrong account.</strong>
                        </li>
                        <li>
                            <strong>People were frustrated by late alerts or no alerts at all when it came to bills, loans, or suspicious transactions.</strong>
                        </li>
                        <li>
                            <strong>Payday changes often led to missed loan payments and NSF charges.</strong>
                        </li>
                        <li>
                            <strong>Users wanted to know when their card was used in a different country, or when someone tried logging in from an unusual IP address.</strong>
                        </li>
                    </ul>
                    <p>
                        Beyond the bank, I launched an ongoing <strong>survey</strong> to better understand what features people actually want in a financial tool. I also wanted to learn how expectations and habits change across age groups and income levels. This research continues as Eden grows, helping us shape the right features through feedback.
                    </p>
                    <p>
                        Through conversations with advisors at other financial institutions and smaller accounting firms, many firms wanted a <strong>SaaS product</strong> that could handle external investments <strong>securely, intelligently</strong>, and in <strong>compliance with government regulations like KYC</strong>.
                    </p>
                    <p>
                        These were the people I was designing for. And these are the voices that helped shape Eden into what it is now.
                    </p>
                </div>
            </div>
        
            <div class="lg:max-w-[500px] xl:max-w-[600px] flex flex-col justify-start gap-2 md:gap-4 xl:gap-6">
                <h2 class="font-safira text-xl leading-[50px] lg:text-2xl xl:text-3xl my-2 xl:my-4">Funding Research</h2>
                 {/* Container div for spacing paragraphs and lists */}
                <div class="flex flex-col gap-4 text-base ">
                    <p>
                        This part was new for me. I wanted to see how a tool like Eden could realistically be funded, so I looked into Canadian and U.S. grant programs.
                    </p>
                    <p>
                        We discovered that Eden could access over <strong>$1.5 million in non-dilutive funding</strong> through a combination of:
                    </p>
                    {/* List 1 with internal spacing AND entire items bolded */}
                    <ul class="list-disc pl-5 flex flex-col gap-4">
                        <li>
                            <strong>Canada's NRC IRAP (Industrial Research Assistance Program)</strong>
                        </li>
                        <li>
                            <strong>Innovative Solutions Canada (early-stage funding and pilot support)</strong>
                        </li>
                        <li>
                            <strong>U.S. SBIR/STTR programs (grants for AI, fintech, and emerging tech)</strong>
                        </li>
                    </ul>
                    <p>
                        These programs fund R&D, product development, and market entry. They're ideal for Eden's mission to <strong>build an AI-powered financial tool</strong> that supports both consumers and institutions.
                    </p>
                    <p>
                        Beyond grants, we also identified:
                    </p>
                    {/* List 2 with internal spacing AND entire items bolded */}
                     <ul class="list-disc pl-5 flex flex-col gap-4">
                        <li>
                            <strong>Accelerators and pitch competitions like Visa Everywhere, MassChallenge, and Elevate Canada that offer cash, mentorship, and investor access.</strong>
                        </li>
                        <li>
                            <strong>University and AI lab partnerships for pilot testing and technical validation.</strong>
                        </li>
                        <li>
                           <strong>Revenue-based financing and forgivable loans from regional agencies as match funding or scaling support.</strong>
                        </li>
                    </ul>
                     <p>
                        The smartest path is to <strong>stack grants and public funds early</strong>, before pursuing equity-based capital. This preserves Eden's independence and gives us time to build real traction.
                    </p>
                </div>
            </div>
        </section>

        <!-- market research -->
        <section id="market-research" class="min-h-screen overflow-hidden flex flex-col gap-8 lg:gap-4  pt-20 pb-28 lg:px-20 px-4 relative max-w-[1700px] mx-auto">
            {/* Main Title */}
            <h2 class="font-safira text-xl leading-[50px] lg:text-2xl xl:text-3xl my-2 xl:my-4">Market Research</h2>

            {/* Introductory Paragraph */}
            <p class="mx-auto lg:mx-0">
                We conducted an <strong>in-depth analysis</strong> of leading personal finance platforms in <strong>2024-2025</strong>, combining <strong>user experience reviews, pricing models, product focus, and target demographics</strong>. Our analysis included tools like <strong>Fey, Monarch Money, Mint, and Perplexity Finance</strong>, as well as emerging or niche apps such as <strong>Cleo, Rocket Money, Empower (formerly Personal Capital), YNAB, and Finary</strong>.
            </p>

            {/* Industry Trends Section */}
            <div class="flex flex-col gap-2 mt-8">
                <h3 class="text-lg font-bold mt-4 mb-2 border-gray-200 border-b-[1px] lg:border-b-[0px] lg:mt-0 lg:text-xl xl:text-2xl">Industry Trends</h3>


                {/* Two-Column Grid for Trends */}
                <div class="grid grid-cols-1 lg:grid-cols-2 gap-x-8 xl:gap-x-12 gap-y-6 lg:gap-y-8 text-base ">

                    {/* Left Column Trends */}
                    <ul class="flex flex-col gap-6 lg:gap-8 list-disc list-outide pl-4">
                        <li><strong>AI Financial Assistants:</strong> Platforms like <strong>Fey and Perplexity</strong> are leading the way in AI-driven tools. They simplify research by allowing users to ask financial questions in <strong>plain English</strong> and receive <strong>personalized summaries and updates</strong>. Yet, they <strong>lack full lifecycle support for personal finance</strong>.
                        </li>
                        <li>
                            <strong>Comprehensive Ecosystems:</strong>
                            Tools like <strong>Monarch and Empower</strong> aim to unify <strong>budgeting, goal tracking, and investment analysis</strong>. Monarch even supports <strong>professional advisor collaboration</strong>. However, pricing and complexity limit their accessibility for casual users.
                        </li>
                        <li>
                            <strong>Decline of Free Tools:</strong>
                            With <strong>Mint shutting down in 2024</strong>, the market has shifted away from ad-supported free models. Most visible apps now charge <strong>$5-15/month</strong>. This raises the bar on design, UX, and long-term value.
                        </li>
                    </ul>

                    {/* Right Column Trends */}
                    <ul class="flex flex-col gap-6 lg:gap-8 list-disc list-outside pl-4">
                        <li>
                            <strong>UX & Emotional Engagement:</strong>
                            <strong>Cleo</strong> shows how <strong>tone and emotion</strong> can drive daily usage, while <strong>YNAB</strong> focus on <strong>structure and planning</strong>. Still, few tools truly guide or comfort users emotionally. Most <strong>remain purely functional</strong>.
                        </li>
                        <li>
                            <strong>Gap in Accessibility:</strong>
                            There's a <strong>growing gap for older adults, newcomers to finance, and those with irregular incomes</strong>. Most tools presume financial literacy and tech comfort. <strong>Gig workers, freelancers, and international users</strong> remain underserved.
                        </li>
                        <li>
                            <strong>Fragmentation & Privacy Concerns:</strong>
                            Users often juggle multiple apps for budgeting, investing, and bill tracking. There's still no <strong>widely adopted, seamless all-in-one platform</strong>. In addition, people increasingly want <strong>transparency and control over how their data is used or shared</strong>.
                        </li>
                    </ul>

                </div>
            </div>
        
        </section>

        <!-- competitive analysis -->
        <section id="competitive" class="h-auto min-h-screen flex flex-col gap-6 lg:gap-8 pt-20 pb-28 lg:px-20 relative max-w-[1700px] mx-auto">
            <div class="bg-gray-100 px-4 sm:px-6 lg:px-10 py-6 pt-8 text-sm lg:text-base overflow-x-auto">
                <p class="py-6">Competitive Analysis</p>
                {/* Added overflow-x-auto for smaller screens */}
                <table class="w-full border-collapse">
                    <thead>
                        <tr class="text-left">
                            {/* Updated table headers */}
                            <th class="bg-gray-100 py-4 px-4 max-w-[40px] min-w-[200px] font-semibold">Platform</th> {/* Made first column sticky */}
                            <th class="bg-gray-100 py-4 px-4 max-w-[40px] min-w-[200px] font-semibold">Core Focus</th>
                            <th class="bg-gray-100 py-4 px-4 max-w-[40px] min-w-[200px] font-semibold">Strengths</th>
                            <th class="bg-gray-100 py-4 px-4 max-w-[40px] min-w-[200px] font-semibold">Gaps/Challenges</th>
                        </tr>
                    </thead>
                    <tbody>
                        {/* Updated table rows with data from the image */}
                        <tr class="bg-white"> {/* Use white/gray-100 for alternating */}
                            <td class="py-4 px-4 max-w-[40px] min-w-[200px] bg-white z-10">Fey</td> {/* Match sticky bg */}
                            <td class="py-4 px-4 max-w-[40px] min-w-[200px]">AI-driven investment research</td>
                            <td class="py-4 px-4 max-w-[40px] min-w-[200px]">Real-time stock data, AI summaries, sleek design</td>
                            <td class="py-4 px-4 max-w-[40px] min-w-[200px]">No budgeting tools, assumes financial literacy, desktop-first</td>
                        </tr>
                        <tr class="bg-gray-100">
                            <td class="py-4 px-4 max-w-[40px] min-w-[200px] bg-gray-100 z-10">Monarch Money</td> {/* Match sticky bg */}
                            <td class="py-4 px-4 max-w-[40px] min-w-[200px]">Budgeting, planning, advisor collaboration</td>
                            <td class="py-4 px-4 max-w-[40px] min-w-[200px]">All-in-one dashboard, customizable views, professional use supported</td>
                            <td class="py-4 px-4 max-w-[40px] min-w-[200px]">High subscription cost (~$99/year), may feel overwhelming to casual users</td>
                        </tr>
                        <tr class="bg-white">
